generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
}

//! Schema em inglês pra não dar ruim, Obs: trauma de atividades anteriores...
model Food {
    id        Int      @id @default(autoincrement())
    name      String
    description String
    price    Decimal
    category     String
    available   Boolean @default(true)
    createdAt DateTime @default(now())

    @@map("foods")
}